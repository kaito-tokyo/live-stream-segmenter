{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "dependencies": [
    "boost-stacktrace",
    "boost-thread",
    {
      "name": "cpp-httplib",
      "default-features": false,
      "features": [
        "zlib"
      ]
    },
    {
      "name": "curl",
      "default-features": false,
      "features": [
        "ssl"
      ],
      "platform": "(windows & !uwp) | mingw"
    },
    {
      "name": "curl",
      "default-features": false,
      "features": [
        "wolfssl"
      ],
      "platform": "(uwp | !windows) & !mingw"
    },
    "fmt",
    "nlohmann-json"
  ]
}
